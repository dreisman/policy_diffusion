
; GRAB STATE LEGISLATIVE METADATA FROM SUNLIGHT
data/input/state_metadata.csv <- [-timestamp]
	input/./state_metadata.py | sed -E "s/u?'//g" > $OUTPUT

; CREATE TABLE FOR STATE METADATA
psql/input/state_metadata <- input/state_metadata.sql, data/input/state_metadata.csv [method:psql]


